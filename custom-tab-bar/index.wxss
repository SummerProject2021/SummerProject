._tabbar {
  width: 100%;
  height: 800rpx;
  display: flex;
  justify-content: space-evenly;
  background: #fff;
  font-size: 26rpx;
  color: #999999;
  box-shadow: 0px -7rpx 13rpx 0px rgba(193, 185, 204, 0.13);
}
._tabbar .titem {
  text-align: center;
  width: 25%;
}
._tabbar .titem image {
  display: block;
  margin: auto;
  width: 400rpx;
  height: 350rpx;
}
._tabbar .tCdk {
  color: #84878a;
}
._iPhoneX {
  height: 148rpx;
}